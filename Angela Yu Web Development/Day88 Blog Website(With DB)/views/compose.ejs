<%- include ("partials/header.ejs") %> 
    <h1>Compose</h1>
   <form action="/" method="post">
    <div class="form-group">
        <label for="title">Title</label>
      <input type="title" class="form-control" id="title" aria-describedby="emailHelp" placeholder="Title" name="postTitle">
    </div>
    <div class="form-group">
      <label for="post">Post</label>
      <textarea name="postBody" id="post" rows="8" class="form-control" placeholder="Post"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>
<%- include("partials/footer.ejs") %>